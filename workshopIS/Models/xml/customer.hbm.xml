<?xml version="1.0" encoding="utf-8" ?>
<!-- This file should be set to an embedded resource -->
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="workshopIS.Models" assembly="workshopIS">
  <class name="CCustomer" table="customer">
    <!-- A 32 hex character is our surrogate key. It's automatically generated by NHibernate with the UUID pattern. -->
    <id name="Id">
      <column name="Id"/>
      <generator class="increment"/>
    </id>
    <property name="Phone"/>

    <property name="FirstName" />

    <property name="Surname" />

    <property name="Email" />

    <property name="ContactState" />

    <property name="CreationDate" />

    <!--<property name="PartnerId" />-->

    <many-to-one name="Partner" column="PartnerId" cascade="save-update"  />

  </class>
</hibernate-mapping>